			/* --- HIDDEN ELEMENTS --- */
.hide, 
#menu-item-957, 
#menu-item-610 { 
	display: none 
}

.sec-text .main-header:after {
	display: none;
}

/* --- HEADER --- */

.nav-wrap [class^="logo"] {
    max-height: 100px;
    padding: 10px 0;
}

.overlay {
    background: #ed1867;
}

.overlay ul li a {
    color: #fff;
    font-family: "Ultra", serif;
}

.overlay ul li a.active, .overlay ul li a:hover {
    color: #f57e20;
}

.overlay ul li a:after {
    bottom: 0;
    background-color: #fff;
}

.nav-wrap.is-fill .button_container.active span:before, .nav-wrap.is-fill .button_container.active span:after,
.button_container.active span:before,
.button_container.active span:after {
	background-color: #fff;
}

.header-buy-now {
    display: inline-block;
    float: right;
    margin-right: 25px;
    margin-top: 20px;
}

.header-buy-now a.btn-main {
    font-size: 18px !important;
    padding: 5px 20px !important;
}

.smaller .header-buy-now {
    margin-top: 7px;
}

.header-buy-now a.btn-main {
    padding: 0px 20px !important;
}


/* --- TYPOGRPAHY --- */
body {
	font-family: Montserrat;
}

h1, h2, h3, h4, h5, h6, p {
	margin: 0;
	padding: 0;
	letter-spacing: 0;
}

.white, .white * {
	color: #fff;
}

.orange, .orange * {
    color: #f57e20;
}

.pink, .pink * {
    color: #ed1867;
}

.no-transform, .no-transform * {
	text-transform: none;
}

/* --- BUTTONS --- */

.btn-main {
    font-family: "Ultra";
	background-color: #fff !important;
    color: #ed1867 !important;
    border: 2px solid #f57e20 !important;
    padding: 1.5rem 2rem !important;
    font-size: 36px !important;
	  line-height: 36px;
    margin: 0 auto;
    display: table;
}

.btn-main:hover {
	color: #fff !important;
    background-color: #f57e20 !important;
}

/* --- VERTICAL TEXT --- */

.add-rotat-header .add-rotat-header__rotatleft, .add-rotat-header .add-rotat-header__rotatright {
    width: 195px;
    top: 100px;
	  font-family: "Ultra";
    color: #ed1867;
    font-size: 1.2rem !important;
}

.add-rotat-header .add-rotat-header__rotatleft {
    left: -120px;
}

.add-rotat-header .add-rotat-header__rotatright {
    right: -120px;
}

/* --- OVERLAY SECTIONS --- */
.overlay-boxes .row-layout {
    display: table;
    width: 100%;
}

.overlay-boxes__text p {
	padding: 0;
}

.overlay-boxes__text .overlay-boxes__text-inside {
    padding: 75px 50px;
}

.orange-bg .with-bg-right:before, .orange-bg .with-bg-left:before, .orange-bg .with-bg-middle:before {
	right: -3em;
background-color: rgba(244, 126, 32, 0.8);
  height: 70%;
}

.pink-bg .overlay-boxes__text:before {
	content: "";
    position: absolute;
    width: 100%;
    top: 50%;
    bottom: auto;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
    z-index: 1;
	 left: -3em;
   background-color: rgba(253, 27, 123, 0.7);
}


/* --- DELIVERY SECTION --- */

#delivery-section .overlay-boxes__text .overlay-boxes__text-inside {
    padding: 0px 50px;
	  max-width: 665px;
    margin: 0 auto;
}


/* --- APP SECTION --- */

.app-logo-wrapper {
	margin-top: 35px;
}

.app-logo {
	max-width: 200px;
	padding: 5px 0px !important;
	margin: 0px auto !important;
}

/* --- BANNER SECTION --- */

.banner-section .story-bg {
    background-position: center !important;
    background-size: cover;
    background-repeat: no-repeat;
}


/* --- MENU SECTION --- */

.order-button-wrapper {
	padding-top: 75px;
}

.menu-list .row {
    margin: 0;
}

#menu .main-header {
	font-size: 28px;
	color: #ed1867;
    width: auto;
    display: table;
    margin: 0 auto 30px !important;
    padding: 0 5px;
}

.cd-folding-panel .cd-close {
    z-index: 10000;
}

.main-header.add-back:before{
    display: none;
}

.main-header:after {
    top: -10px;
    height: 2.25px;
    background-color: #f57e20;
	
}

#menu .cd-item .cd-item__text, #menu .cd-item .cd-item__inner {
    background-color: #f57e20;
}

.cd-item .overlay-header.alternative {
    font-size: 26px;
}

#menu .cd-item i {
    color: #fff;
    font-size: 30px;
    font-weight: 900;
   	margin-top: 10px;
}

#menu .menu .menu-list {
    padding: 1em 0;
    border-bottom: 3px dotted #f57e20;
	  border-top-width:0px;
    border-right-width:0px;
    border-bottom-width:3px;
    border-left-width:0px;
}

#menu .cd-folding-panel {
    z-index: 10000;
}


#menu .cd-folding-panel .cd-close::after, #menu .cd-folding-panel .cd-close::before {
  height: 7px;
	background-color: #f57e20;
}

#menu .cd-folding-panel a {
	color: #ed1867;
	text-decoration:underline;
}

#menu .cd-folding-panel .section-title {
    color: #ed1867;
    font-weight: 600;
    margin: 0;
    font-size: 30px;
    margin: 20px 0;
	text-transform: uppercase;
}


#menu .menu .menu-list:first-child .section-title {
    margin: 0 0 20px;
}


#menu .cd-folding-panel .item-title {
	color: #ed1867;
	font-weight: 600;
  margin: 0;
	font-size: 20px;
	line-height: 24px;
	text-transform: uppercase;
}

#menu .cd-folding-panel .menu-notice {
	font-weight: 700;
	font-size: 12px;
	line-height: 12px;
}

#menu .cd-folding-panel .item-description {
	color: #ed1867;
  margin: 2px 0 0;
	padding: 0;
	font-weight: 400;
	font-size: 20px;
	line-height: 24px;
	text-transform: uppercase;
}

#menu .cd-folding-panel .item-addon {
	color: #ed1867;
  margin: 10px 0 0;
	padding: 0;
	font-weight: 600;
	font-size: 16px;
	line-height: 20px;
	text-transform: uppercase;
}

#menu .cd-folding-panel .menu-price {
	color: #ed1867;
	font-weight: 600;
  margin: 0;
	font-size: 20px;
	line-height: 24px;
	text-transform: uppercase;
	font-family: "Montserrat";
}

#menu .cd-folding-panel .cd-close {
    top: 50px;
}


/* -- WHO WE ARE SECTION -- */

#who-we-are h1 {
}

#who-we-are .deco-text {
	margin: 20px 0 10px;
}

#who-we-are p {
	font-weight: 600;
